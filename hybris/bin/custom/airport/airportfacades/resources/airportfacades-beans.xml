<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="beans.xsd"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean
		class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders"
			type="java.util.List&lt;it.uiip.airport.facades.product.data.GenderData>" />
	</bean>

	<bean class="it.uiip.airport.facades.product.data.GenderData">
		<property name="code" type="String" />
		<property name="name" type="String" />
	</bean>
	
	<bean class="it.uiip.airport.facades.data.AirportData">
		<property name="code" type="String"/>
		<property name="city" type="String"/>
		<property name="country" type="String"/>
		<property name="nRunways" type="Integer"/>
	</bean>
	

	<bean class="it.uiip.airport.facades.data.PlaneData">
		<property name="code" type="String"/>
		<property name="nPassengers" type="Integer"/>
		<property name="nGoods" type="Integer"/>
	</bean>

	<bean class="it.uiip.airport.facades.data.TicketsData">
		<property name="code" type="String"/>
		<property name="numberSeat" type="String"/>
		<property name="passenger" type="PassengerData"/>
	</bean>
	
	<bean class="it.uiip.airport.facades.data.FlightData">
		<property name="code" type="String"/>
		<property name="dayWeek" type="String"/>
		<property name="departureTime" type="String"/>
		<property name="timeArrival" type="String"/>
		<property name="departureAirport" type="AirportData"/>
		<property name="arrivalAirport" type="AirportData"/>
	</bean>


	<bean class="it.uiip.airport.facades.data.PassengerData">
		<property name="uid" type="String"/>
		<property name="name" type="String"/>
		<property name="surname" type="String"/>
		<property name="passport" type="String"/>
		<property name="ticket" type="TicketsData"/>
		<property name="flight" type="FlightData"/>
	</bean>

<!--	<bean class="it.uiip.airport.facades.data.PassengerData2">
		<property name="uid" type="String"></property>
		<property name="name" type="String"></property>
		<property name="surname" type="String"></property>
		<property name="passport" type="String"></property>
	</bean> -->
</beans>
