<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype code="GenderList" elementtype="Gender"
			autocreate="true" generate="true" type="list" />
		<collectiontype code="SwatchColorSet"
			elementtype="SwatchColorEnum" autocreate="true" generate="true"
			type="set" />
	</collectiontypes>



	<enumtypes>
		<enumtype generate="true" code="SwatchColorEnum"
			autocreate="true" dynamic="true">
			<value code="BLACK" />
			<value code="BLUE" />
			<value code="BROWN" />
			<value code="GREEN" />
			<value code="GREY" />
			<value code="ORANGE" />
			<value code="PINK" />
			<value code="PURPLE" />
			<value code="RED" />
			<value code="SILVER" />
			<value code="WHITE" />
			<value code="YELLOW" />
		</enumtype>
	</enumtypes>


	<relations>

		<relation generate="true" localized="false"
			code="Plane2FlightRelation" autocreate="true">

			<sourceElement type="Plane" qualifier="plane"
				cardinality="one"></sourceElement>

			<targetElement type="Flight" qualifier="planes"
				cardinality="many"></targetElement>

		</relation>

		<relation localized="false"
			code="DepartureAirport2FlightRelation" generate="true"
			autocreate="true">


			<sourceElement type="Airport"
				qualifier="departureAirport" cardinality="one"></sourceElement>

			<targetElement type="Flight"
				qualifier="departureAirports" cardinality="many"></targetElement>

		</relation>

		<relation localized="false"
			code="AirportArrival2FlightRelation">

			<sourceElement type="Airport"
				qualifier="airportArrival" cardinality="one"></sourceElement>

			<targetElement type="Flight"
				qualifier="airportArrivals" cardinality="many"></targetElement>

		</relation>

		<relation generate="true" autocreate="true" localized="false"
			code="Flight2PassengerRelation">
			<sourceElement qualifier="flight" type="Flight"
				cardinality="one"></sourceElement>
			<targetElement qualifier="flights" type="Passenger"
				cardinality="many"></targetElement>
		</relation>

	</relations>

	<itemtypes>

		<!-- Add your item definitions here -->


		<typegroup name="Airport">

			<itemtype code="Airport" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.Airport">
				<description>Airport product that contain attribute</description>
				<deployment table="Airport" typecode="11000" />

				<attributes>

					<attribute type="java.lang.String" qualifier="code"
						autocreate="true">
						<description>Attribute code</description>
						<modifiers read="true" write="true" unique="true"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="localized:java.lang.String"
						qualifier="city" autocreate="true">
						<description>Attribute city</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="localized:java.lang.String"
						qualifier="country" autocreate="true">
						<description>Attribute country</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.Integer" qualifier="nRunways">
						<description>Attribute nRunways</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

				</attributes>

			</itemtype>

			<itemtype code="Plane" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.Airplane">
				<description>Airplane product that contain attribute</description>
				<deployment table="Plane" typecode="11001" />

				<attributes>

					<attribute type="java.lang.String" qualifier="code"
						autocreate="true">
						<description>primary key of table plane</description>
						<modifiers read="true" write="true" optional="false"
							unique="true" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.Integer"
						qualifier="nPassengers">
						<description>Attribute number of passengers</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.Integer" qualifier="nGoods">
						<description>Attribute number of goods</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

				</attributes>

			</itemtype>

			<itemtype code="Flight" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.Flight">
				<description>Flight product that contain attribute</description>
				<deployment table="Flight" typecode="11002" />

				<attributes>

					<attribute type="java.lang.Integer" qualifier="code">
						<description>Primary key of table Flight</description>
						<modifiers read="true" write="true" optional="false"
							unique="true" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="localized:java.lang.String"
						qualifier="dayWeek">
						<description>Attribute day of the week</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="codeDepartureAirport">
						<description>Attribute Airport of departure</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="codeAirportArrival">
						<description>Attribute Airport of arrival</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="codeAirplane">
						<description>Attribute type of plane</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="departureTime">
						<description>Attribute of departure time</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="timeArrival">
						<description>Attribute of time arrival</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

				</attributes>

			</itemtype>

			<itemtype code="Passenger" extends="User" autocreate="true"
				generate="true" jaloclass="it.uiip.airport.core.jalo.Passenger">

				<description>Passenger product that contain attribute</description>

				<deployment table="Passenger" typecode="11003" />

				<attributes>
				
				<attribute type="java.lang.Integer" qualifier="uid"
						autocreate="true">
						<description>Attribute id</description>
						<modifiers read="true" write="true" unique="true"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String" qualifier="name">
						<description>Attribute of the name of the passenger</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String" qualifier="surname">
						<description>Attribute of the surname of the passenger</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String" qualifier="passport">
						<description>Attribute of the alphanumeric code of the passport</description>
						<modifiers read="true" write="true" optional="false" />
						<persistence type="property"></persistence>
					</attribute>
					<attribute type="java.lang.Integer" qualifier="fligh">
						<description>Primary key of table Flight</description>
						<modifiers read="true" write="true" optional="false"
							unique="true" />
						<persistence type="property"></persistence>
					</attribute>
					
				</attributes>
			</itemtype>
		</typegroup>







		<!-- TypeGroups are for structure of this file only -->

		<typegroup name="Apparel">
			<itemtype code="ApparelProduct" extends="Product"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelProduct">
				<description>Base apparel product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="genders" type="GenderList">
						<description>List of genders that the ApparelProduct is designed for</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>

			<itemtype code="ApparelStyleVariantProduct"
				extends="VariantProduct" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelStyleVariantProduct">
				<description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
				<attributes>
					<attribute qualifier="style"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color/Pattern of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>

					<attribute qualifier="swatchColors" type="SwatchColorSet">
						<description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>

			</itemtype>

			<itemtype code="ApparelSizeVariantProduct"
				extends="ApparelStyleVariantProduct" autocreate="true"
				generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelSizeVariantProduct">
				<description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
				<attributes>
					<attribute qualifier="size"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="Electronics">
			<itemtype code="ElectronicsColorVariantProduct"
				extends="VariantProduct" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ElectronicsColorVariantProduct">
				<description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
				<attributes>
					<attribute qualifier="color"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

	</itemtypes>
</items>
