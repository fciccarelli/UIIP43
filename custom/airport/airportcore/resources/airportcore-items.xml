<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype code="GenderList" elementtype="Gender"
			autocreate="true" generate="true" type="list" />
		<collectiontype code="SwatchColorSet"
			elementtype="SwatchColorEnum" autocreate="true" generate="true"
			type="set" />
	</collectiontypes>

	<enumtypes>
		<enumtype generate="true" code="SwatchColorEnum"
			autocreate="true" dynamic="true">
			<value code="BLACK" />
			<value code="BLUE" />
			<value code="BROWN" />
			<value code="GREEN" />
			<value code="GREY" />
			<value code="ORANGE" />
			<value code="PINK" />
			<value code="PURPLE" />
			<value code="RED" />
			<value code="SILVER" />
			<value code="WHITE" />
			<value code="YELLOW" />
		</enumtype>
	</enumtypes>
	
	<relations>
		<relation generate="true" localized="true" code="Plane2FlightRelation" autocreate="true">
			<sourceElement type="Plane" qualifier="code" cardinality="one"></sourceElement>
			<targetElement type="Flight" qualifier="codePlanes" cardinality="many"></targetElement>
		</relation>
		<relation  generate="true" localized="true" code="DepartureAirport2FlightRelation" autocreate="true">
			<sourceElement type="Airport" qualifier="code" cardinality="one"></sourceElement>
			<targetElement type="Flight" qualifier="codeDepartureAirports" cardinality="many"></targetElement>
		</relation>
		<relation generate="true" localized="true" code="AirportArrival2FlightRelation" autocreate="true">
			<sourceElement type="Airport" qualifier="code" cardinality="one"></sourceElement>
			<targetElement type="Flight" qualifier="codeAirportArrivals" cardinality="many"></targetElement>
		</relation>
	</relations>
	

	<itemtypes>

		<!-- Add your item definitions here -->


		<!-- TypeGroups are for structure of this file only -->

		<typegroup name="Airport">
			<itemtype code="Airport"
			autocreate="true" generate="true" jaloclass="it.uiip.airport.core.jalo.Airport" >
				<description>Airport product that contains attribute</description>
				<deployment table="Airport" typecode="11000"/>
					<attributes>
						<attribute type="java.lang.String" qualifier="code">
						<description>Attribute code airport</description>
						<modifiers read="true" write="true" unique="true" optional="false"/>
						<persistence type="property"></persistence>
						</attribute>
						<attribute type="java.lang.String" qualifier="city">
						<description>Attribute city</description>
						<modifiers read="true" write="true"  optional="false"/>
						<persistence type="property"></persistence>
						</attribute>
						<attribute type="java.lang.String" qualifier="country">
						<description>Attribute country</description>
						<modifiers read="true" write="true"  optional="false"/>
						<persistence type="property"></persistence>
						</attribute>
						<attribute type="java.lang.Integer" qualifier="nRunways">
						<description>Attribute number of runways</description>
						<modifiers read="true" write="true"  optional="false"/>
						<persistence type="property"></persistence>
						</attribute>
					</attributes>
			</itemtype>
			
			<itemtype code="Plane" autocreate="true" generate="true" jaloclass="it.uiip.airport.core.jalo.Plane">
			<description>Plane product that contains attribute</description>
			<deployment table="Plane" typecode="11001"/>
				<attributes>
					<attribute type="java.lang.String" qualifier="code">
					<description>Attribute code plane</description>
					<modifiers read="true" write="true" unique="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
					<attribute type="java.lang.Integer" qualifier="nPassengers">
					<description>Attribute number of passengers plane</description>
					<modifiers read="true" write="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
					<attribute type="java.lang.Integer" qualifier="nGoods">
					<description>Attribute number of goods</description>
					<modifiers read="true" write="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="Flight" autocreate="true" generate="true" jaloclass="it.uiip.airport.core.jalo.Flight">
			<description>Flight product that contains attribute</description>
			<deployment table="Flight" typecode="11002"/>
				<attributes>
					<attribute type="java.lang.Integer" qualifier="code">
					<description>Attribute code flight</description>
					<modifiers read="true" write="false" unique="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
					<attribute type="java.lang.String" qualifier="dayWeek">
					<description>Attribute day of week</description>
					<modifiers read="true" write="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
					<attribute type="java.lang.String" qualifier="codeDepartureAirport">
					<description>Attribute departure airport</description>
					<modifiers read="true" write="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
					<attribute type="java.lang.String" qualifier="codeAirportArrival">
					<description>Attribute arrival airport</description>
					<modifiers read="true" write="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
				  	<attribute type="java.lang.String" qualifier="codePlane">
					<description>Attribute type of plane</description>
					<modifiers read="true" write="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
					<attribute type="java.lang.String" qualifier="departureTime">
					<description>Attribute departure time</description>
					<modifiers read="true" write="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
					<attribute type="java.lang.String" qualifier="timeArrival">
					<description>Attribute time arrival</description>
					<modifiers read="true" write="true" optional="false"/>
					<persistence type="property"></persistence>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="Apparel">
			<itemtype code="ApparelProduct" extends="Product"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelProduct">
				<description>Base apparel product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="genders" type="GenderList">
						<description>List of genders that the ApparelProduct is designed for</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>

			<itemtype code="ApparelStyleVariantProduct"
				extends="VariantProduct" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelStyleVariantProduct">
				<description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
				<attributes>
					<attribute qualifier="style"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color/Pattern of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>

					<attribute qualifier="swatchColors" type="SwatchColorSet">
						<description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>

			</itemtype>

			<itemtype code="ApparelSizeVariantProduct"
				extends="ApparelStyleVariantProduct" autocreate="true"
				generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelSizeVariantProduct">
				<description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
				<attributes>
					<attribute qualifier="size"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="Electronics">
			<itemtype code="ElectronicsColorVariantProduct"
				extends="VariantProduct" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ElectronicsColorVariantProduct">
				<description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
				<attributes>
					<attribute qualifier="color"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>


	</itemtypes>
</items>
