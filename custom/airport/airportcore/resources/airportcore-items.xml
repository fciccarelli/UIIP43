<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype code="GenderList" elementtype="Gender"
			autocreate="true" generate="true" type="list" />
		<collectiontype code="SwatchColorSet"
			elementtype="SwatchColorEnum" autocreate="true" generate="true"
			type="set" />
	</collectiontypes>

	<enumtypes>
		<enumtype generate="true" code="SwatchColorEnum"
			autocreate="true" dynamic="true">
			<value code="BLACK" />
			<value code="BLUE" />
			<value code="BROWN" />
			<value code="GREEN" />
			<value code="GREY" />
			<value code="ORANGE" />
			<value code="PINK" />
			<value code="PURPLE" />
			<value code="RED" />
			<value code="SILVER" />
			<value code="WHITE" />
			<value code="YELLOW" />
		</enumtype>
	</enumtypes>
	
	<relations>
		<relation code="Plane2FlightRelation" generate="true"
			localized="false" autocreate="true">
			<sourceElement type="Plane" cardinality="one"
				qualifier="code">
				<!-- <modifiers read="true" write="true" optional="false"/> -->
			</sourceElement>
			<targetElement type="Flight" cardinality="many"
				qualifier="codePlanes">
			</targetElement>
		</relation>

		<relation code="DepartureAirport2FlightRelation"
			generate="true" localized="false" autocreate="true">
			<sourceElement type="Airport" cardinality="one"
				qualifier="code">
			</sourceElement>
			<targetElement type="Flight" cardinality="many"
				qualifier="codeDepartureAirports">
			</targetElement>
		</relation>

		<relation code="AirportArrival2FlightRelation"
			generate="true" localized="false" autocreate="true">
			<sourceElement type="Airport" cardinality="one"
				qualifier="code">
			</sourceElement>
			<targetElement type="Flight" cardinality="many"
				qualifier="codeAirportArrivals">
			</targetElement>
		</relation>

	</relations>
	
	
	<itemtypes>

		

		<!-- Add your item definitions here -->


		<!-- TypeGroups are for structure of this file only -->

		<typegroup name="Apparel">
			<itemtype code="ApparelProduct" extends="Product"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelProduct">
				<description>Base apparel product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="genders" type="GenderList">
						<description>List of genders that the ApparelProduct is designed for</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>

			<itemtype code="ApparelStyleVariantProduct"
				extends="VariantProduct" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelStyleVariantProduct">
				<description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
				<attributes>
					<attribute qualifier="style"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color/Pattern of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>

					<attribute qualifier="swatchColors" type="SwatchColorSet">
						<description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>

			</itemtype>

			<itemtype code="ApparelSizeVariantProduct"
				extends="ApparelStyleVariantProduct" autocreate="true"
				generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelSizeVariantProduct">
				<description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
				<attributes>
					<attribute qualifier="size"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="Electronics">
			<itemtype code="ElectronicsColorVariantProduct"
				extends="VariantProduct" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ElectronicsColorVariantProduct">
				<description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
				<attributes>
					<attribute qualifier="color"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="Airport">
			<itemtype code="Airport" extends="GenericItem"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.Airport">

				<description> Airport type that models an airport </description>

				<deployment table="Airport" typecode="20000" />

				<attributes>

					<attribute qualifier="code" type="java.lang.String">
						<description> Id airport primary key </description>
						<modifiers read="true" write="false" unique="true"
							optional="false" />
						<persistence type="property" />
					</attribute>

					<attribute qualifier="city" type="java.lang.String">
						<description> Airport city </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property" />
					</attribute>

					<attribute qualifier="country" type="java.lang.String">
						<description> Airport country </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property" />
					</attribute>

					<attribute qualifier="nRunways" type="java.lang.Integer">
						<description> Airport runways number </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property" />
					</attribute>

				</attributes>

			</itemtype>

			<itemtype code="Plane" extends="GenericItem"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.Plane">

				<description> Plane type that models an airplane </description>

				<deployment table="Plane" typecode="20001" />

				<attributes>

					<attribute type="java.lang.String" qualifier="code">
						<description> Plane type </description>
						<modifiers read="true" write="false" unique="true"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.Integer"
						qualifier="nPassengers">
						<description> Number of the passengers </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.Integer" qualifier="nGoods">
						<description> Plane type </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>
				</attributes>

			</itemtype>

			<itemtype code="Flight" extends="GenericItem"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.Flight">

				<description> Flight type that models a flight </description>

				<deployment table="Flight" typecode="20002" />

				<attributes>

					<attribute type="java.lang.Integer" qualifier="code">
						<description> Flight type </description>
						<modifiers read="true" write="false" unique="true"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String" qualifier="dayWeek">
						<description> Day of the week </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="codeDepartureAirport">
						<description> Departure airport </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="codeAirportArrival">
						<description> Airport of arrival </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String" qualifier="codePlane">
						<description> The type of the plane </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="departureTime">
						<description> The time of departure </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

					<attribute type="java.lang.String"
						qualifier="timeArrival">
						<description> The time of arrival </description>
						<modifiers read="true" write="true" unique="false"
							optional="false" />
						<persistence type="property"></persistence>
					</attribute>

				</attributes>

			</itemtype>
		</typegroup>

	</itemtypes>
</items>
